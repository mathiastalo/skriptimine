#! /bin/bash
#
read -p "Sisesta oma vanus: " vanus   #Laseb sisestada vanuse
#Hakkab testima vanust if commandiga
if test $vanus -gt 0 -a $vanus -lt 11
	then
		echo "Oled laps"
	elif test $vanus -ge 11 -a $vanus -lt 18
	then
		echo "Oled nooruk"
	elif test $vanus -ge 18 -a $vanus -lt 63
	then
		echo "Oled tÃ¤iskasvanu"
	elif test $vanus  -ge 63
	then
		echo "Oled seenior"
	else
		echo "Vigane sisend"
	fi
